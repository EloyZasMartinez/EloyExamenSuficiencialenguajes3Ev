<div class="accordion">
    <% for (var i = 0; i < actor_details.length; i++) { %>
    <!-- Each accordion item -->
    <div class="accordionitem">
        <!-- Heading -->
        <div class="accordionheader">
            <!-- The toggle icon -->
            <div class="accordiontoggle"></div>

            <!-- The title -->
            <div class="accordiontitle">
                <%= actor[i].name %>
            </div>
        </div>

        <!-- The content -->
        <div class="accordion__content">
            <%= actor[i].place_of_birth %>
        </div>
    </div>
    
    <% } %>


    
</div>